\nclass Counter {\n    constructor() {\n        this.sec = 0;\n    }\n\n    add() {\n        return this.sec += 10\n    }\n    getVal() {\n        return this.sec;\n    }\n    reset() {\n        this.sec = 0;\n    }\n}\n\nconst Tes = new Counter();\n\nconst initialTime = {\n    time: Tes.getVal(),\n    isRunning: false\n}\n\nconst TimeReducer = (state, action) => {\n    switch (action.type) {\n        case \"START\":\n            return {\n                ...state, isRunning: true\n            }\n        case \"STOP\":\n            return {\n                ...state,\n                isRunning: action.value\n            }\n        case \"RESET\":\n            Tes.reset()\n            return {\n                time: Tes.getVal(),\n                isRunning: false\n            }\n        case \"TICK\":\n            return {\n                ...state,\n                time: action.value\n            }\n        default:\n            throw new Error();\n    }\n\n}\n\n\nfunction Timer() {\n\n    const [TimeState, dispatchTime] = useReducer(TimeReducer, initialTime)\n\n    useEffect(() => {\n        if (!TimeState.isRunning) {\n\n            return;\n        }\n\n        var SetIntrvl = setInterval(() => {\n            dispatchTime({\n                type: \"TICK\",\n                value: Tes.add()\n            })\n        }, 1000)\n\n\n        return (() => {\n            console.log(Tes.getVal())\n            clearInterval(SetIntrvl)\n            console.log('clearance');\n        })\n    }, [TimeState.isRunning])\n\n\n    const startHandler = () => {\n        dispatchTime(\n            {\n                type: \"START\",\n            }\n        )\n    }\n    const stopHandler = () => {\n        dispatchTime(\n            {\n                type: \"STOP\",\n                isRunning: false\n            }\n        )\n    }\n    const resetHandler = () => {\n        dispatchTime(\n            {\n                type: \"RESET\",\n            }\n        )\n    }\n\n    return (\n        <div>\n            <nav id={''}>\n                <ul id={styles.ulWrapper}>\n                    <li>{TimeState.time}</li>\n                    <li>List B</li>\n                </ul>\n            </nav>\n            <div>\n                <Button onClick={startHandler}>start</Button>\n                <Button onClick={stopHandler}>stop</Button>\n                <Button onClick={resetHandler}>reset</Button>\n\n            </div>\n        </div >\n    )\n}\n\nexport default Timer;","/Users/macbook/learn_reactjs/react_learn/src/UI/Button/Button.js",[],"/Users/macbook/learn_reactjs/react_learn/src/components/CounterUseReducer/Counter.js",[],{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","severity":1,"message":"56","line":1,"column":17,"nodeType":"57","messageId":"58","endLine":1,"endColumn":25},"no-native-reassign",["59"],"no-negated-in-lhs",["60"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]